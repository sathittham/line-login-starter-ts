# LINE Login credentials (get these from your LINE Developers Console)
LINE_CLIENT_ID=your_line_client_id
LINE_CLIENT_SECRET=your_line_client_secret
LINE_REDIRECT_URI=http://localhost:5000/api/auth/line/callback

# Session secret for express-session (use a long random string)
SESSION_SECRET=your_session_secret

# Server port
PORT=5000

# Frontend URLs for redirect after login
FRONTEND_SUCCESS_URL=http://localhost:3000/success
FRONTEND_ERROR_URL=http://localhost:3000/error